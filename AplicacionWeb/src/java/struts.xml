<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <package name="paquete" extends="struts-default">
        
        <action name="ingreso" method="execute" class="paquete.Ingreso">
            <result name="Profesor" >profesor.jsp</result>
            <result name="Alumno" >alumno.jsp</result>
            <result name="Administrador" >administrador.jsp</result>
            <result name="Fail" >fallo.jsp</result>
        </action>
        
    </package>
</struts>
