<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletRopa</servlet-name>
        <servlet-class>paquete.ServletRopa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveProducts</servlet-name>
        <servlet-class>paquete.SaveProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletComida</servlet-name>
        <servlet-class>paquete.ServletComida</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletJuegos</servlet-name>
        <servlet-class>paquete.ServletJuegos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Comprar</servlet-name>
        <servlet-class>paquete.Comprar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLoggin</servlet-name>
        <servlet-class>Ingreso.ServletLoggin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginBean</servlet-name>
        <servlet-class>Ingreso.LoginBean</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletRopa</servlet-name>
        <url-pattern>/ServletRopa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveProducts</servlet-name>
        <url-pattern>/SaveProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletComida</servlet-name>
        <url-pattern>/ServletComida</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletJuegos</servlet-name>
        <url-pattern>/ServletJuegos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Comprar</servlet-name>
        <url-pattern>/Comprar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLoggin</servlet-name>
        <url-pattern>/ServletLoggin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginBean</servlet-name>
        <url-pattern>/LoginBean</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
